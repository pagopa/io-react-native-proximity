{
  "name": "io-react-native-proximity-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace IoReactNativeProximityExample.xcworkspace -scheme IoReactNativeProximityExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO",
    "postinstall": "patch-package && rn-nodeify --install process,path,buffer,crypto,util,stream,constants,events,url,fs"
  },
  "dependencies": {
    "buffer": "^4.9.2",
    "cbor-x": "^1.5.4",
    "constants-browserify": "^1.0.0",
    "events": "^1.1.1",
    "path-browserify": "^0.0.0",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-ble-manager": "^10.1.5",
    "react-native-crypto": "^2.2.0",
    "react-native-level-fs": "^3.0.1",
    "react-native-randombytes": "^3.6.1",
    "react-native-uuid": "^2.0.1",
    "readable-stream": "^1.0.33",
    "rn-qr-generator": "^1.3.1",
    "stream-browserify": "^1.0.0",
    "text-encoding-polyfill": "^0.6.7",
    "url": "^0.10.3",
    "util": "^0.10.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/metro-config": "^0.72.11",
    "babel-plugin-module-resolver": "^5.0.0",
    "metro-react-native-babel-preset": "0.76.8",
    "patch-package": "^8.0.0",
    "pod-install": "^0.1.0",
    "rn-nodeify": "^10.3.0"
  },
  "engines": {
    "node": ">=16"
  },
  "react-native": {
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "path": "path-browserify",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "browser": {
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "_stream_readable": "readable-stream/readable",
    "_stream_transform": "readable-stream/transform",
    "_stream_writable": "readable-stream/writable",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "fs": "react-native-level-fs",
    "path": "path-browserify",
    "stream": "stream-browserify"
  }
}
